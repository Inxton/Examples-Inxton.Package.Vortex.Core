<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.2">
  <POU Name="fbWorldWeatherWatch" Id="{93bf999a-20b2-4c2f-97ca-b40c4de722c6}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK fbWorldWeatherWatch
VAR
	{attribute addProperty Name "<#North pole station#>"}
	NorthPole : structWeatherStation := (StationICAO := 'BINP');
	{attribute addProperty Name "<#South pole station#>"}
	SouthPole : structWeatherStation := (StationICAO := 'NZSP');
	{attribute addProperty Name "<#Verl, Germany#>"}
	Verl : structWeatherStation := (StationICAO := 'EDLP');
	{attribute addProperty Name "<#Kriva, Slovakia#>"}
	Kriva : structWeatherStation := (StationICAO := 'LZIB');
	
	{attribute addProperty Name "<#What plc says#>"}
	PlcCommentOnCurrentWeather : STRING(255);
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[(* PLC IS COMMENTING ON WEATHER, GETTING INFORMATION FROM WEATHER STATIONS FROM ARROUND THE WORLD *)

Utils.FluentString.Construct().Append('So, the weather situation is following: ');

IF (NorthPole.StationStatus = enumStationStatus.Available) THEN
	IF(NorthPole.Temp > -50) THEN
		Utils.FluentString.Append('It is fairly warm at the north pole. ');
	ELSE
		Utils.FluentString.Append('It is as cold as it should be at the nort pole. ');
	END_IF;
ELSE;
	Utils.FluentString.Append('North pole station is not available. ');
END_IF; 


IF (SouthPole.StationStatus = enumStationStatus.Available) THEN
	IF(SouthPole.Temp > -50) THEN
		Utils.FluentString.Append('It is fairly warm at the south pole. ');
	ELSE
		Utils.FluentString.Append('It is as cold as it should be at the souht pole. ');
	END_IF; 
ELSE;
	Utils.FluentString.Append('South pole station is not available. ');
END_IF; 


IF (Verl.StationStatus = enumStationStatus.Available) THEN
	IF(Verl.Temp > 0) THEN
		Utils.FluentString.Append('It is not freezing in Verl. ');
	ELSE
		Utils.FluentString.Append('It is freezing in Verl. ');
	END_IF; 
ELSE;
	Utils.FluentString.Append('Verl station is not available. ');
END_IF; 


IF (Kriva.StationStatus = enumStationStatus.Available) THEN
	IF(Kriva.Temp > 0) THEN
		Utils.FluentString.Append('It is not freezing in Kriva. ');
	ELSE
		Utils.FluentString.Append('It is freezing in Kriva. ');
	END_IF; 
ELSE;
	Utils.FluentString.Append('Kriva station is not available. ');
END_IF; 

PlcCommentOnCurrentWeather := Utils.FluentString.Result;


]]></ST>
    </Implementation>
